{
    "name": "fmo/guess-game",
    "description": "Game API",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.8",
        "lexik/jwt-authentication-bundle": "^v3.1.0",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "6.4.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/security-bundle": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation-contracts": "^2.3",
        "symfony/yaml": "6.4.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Guess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guess\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "./vendor/bin/phpunit tests/"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*",
            "docker": true
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9"
    }
}
